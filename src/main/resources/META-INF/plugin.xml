<idea-plugin>

    <id>codeemoji-plugin</id>
    <name><![CDATA[codÆŽEmoji]]></name>
    <version>1.1.0-SNAPSHOT</version>
    <vendor email="eduardo.martinsguerra@unibz.it" url="https://www.unibz.it">Free University of Bozen-Bolzano - UNIBZ</vendor>

    <resource-bundle>CEBundle</resource-bundle>

    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <applicationService serviceImplementation="codeemoji.inlay.nameviolation.ShortDescriptiveNameSettings"/>
        <applicationService serviceImplementation="codeemoji.inlay.showingmodifiers.ShowingModifiersSettings"/>
        <applicationService serviceImplementation="codeemoji.inlay.showingspecifics.ShowingSpecificsSettings"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.nameviolation.ShortDescriptiveName"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.nameviolation.GetMoreThanAccessor"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.nameviolation.IsReturnsMoreThanABoolean"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.nameviolation.SetMethodReturns"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.nameviolation.ExpectingButNotGettingASingleInstance"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.nameviolation.ValidationMethodDoesNotConfirm"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.nameviolation.GetMethodDoesNotReturn"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.nameviolation.NotAnsweredQuestion"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.nameviolation.TransformMethodDoesNotReturn"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.nameviolation.ExpectingButNotGettingACollection"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.nameviolation.SaysOneButContainsMany"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.nameviolation.NameSuggestsBooleanByTypeDoesNot"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.nameviolation.SaysManyButContainsOne"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.nameviolation.NameContainsOnlySpecialCharacters"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.showingmodifiers.ShowingModifiers"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.showingspecifics.ShowingSpecifics"/>
    </extensions>
</idea-plugin>