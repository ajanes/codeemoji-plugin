<idea-plugin>

    <id>codeemoji-plugin</id>
    <name><![CDATA[codÆŽEmoji]]></name>
    <version>1.0.0-SNAPSHOT</version>
    <vendor email="fernando.opc@gmail.com" url="https://www.unibz.it">UNIBZ</vendor>

    <resource-bundle>CEBundle</resource-bundle>

    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <applicationService serviceImplementation="codeemoji.inlay.BadVariableNameSettings"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.BadVariableName"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.IsReturnsMoreThanABoolean"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.SetMethodReturns"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.GetMethodDoesNotReturn"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.GetMoreThanAccessor"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.ExpectingButNotGettingASingleInstance"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.ValidationMethodDoesNotConfirm"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.NotAnsweredQuestion"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.TransformMethodDoesNotReturn"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.ExpectingButNotGettingACollection"/>
        <codeInsight.inlayProvider language="JAVA" implementationClass="codeemoji.inlay.SaysOneButContainsMany"/>
    </extensions>
</idea-plugin>